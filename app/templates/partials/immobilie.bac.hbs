                <article class="stripe">
                    {{#if [expose.expose]}}
                        <div class="row">
                            <div class="col x8">
                                <div class="imgal">
                                    <figure class="imgal__figure imgal__figure--large">
                                        <a id="imgal__openlink" href="{{replaceStr [expose.expose].realEstate.titlePicture.urls.0.url.4.[@href] '%WIDTH%' '800' '%HEIGHT%' '600'}}">
                                            <img id="imgal-img" class="imgal__img" width="570" height="429" alt="" src="{{replaceStr [expose.expose].realEstate.titlePicture.urls.0.url.4.[@href] '%WIDTH%' '570' '%HEIGHT%' '429'}}">
                                        </a>
                                        <!--figcaption class="imgal__caption">{{[expose.expose].realEstate.titlePicture.title}}</figcaption-->
                                    </figure>
                                    <div class="imgal__thumbs clearfix">
                                        {{#each [expose.expose].realEstate.attachments}}
                                            {{#each this.attachment}}
                                                <figure class="imgal__figure">
                                                    <img data-imgal-medium="{{replaceStr this.urls.0.url.4.[@href] '%WIDTH%' '570' '%HEIGHT%' '429'}}" data-imgal-zoom="{{replaceStr this.urls.0.url.4.[@href] '%WIDTH%' '800' '%HEIGHT%' '600'}}" class="imgal__img imgal__img--thumb" width="80" height="60" alt="" src="{{replaceStr this.urls.0.url.4.[@href] '%WIDTH%' '80' '%HEIGHT%' '60'}}">
                                                    <!--figcaption>{{this.title}}</figcaption-->
                                                </figure>
                                            {{/each}}
                                        {{/each}}
                                    </div>
                                </div>
                                {{#if [expose.expose].realEstate.lodgerFlat}}
                                    {{#contains [expose.expose].realEstate.lodgerFlat "YES"}}
                                        <span class="keyword">Einliegerwohnung</span>
                                    {{/contains}}
                                {{/if}}
                                {{#if [expose.expose].realEstate.cellar}}
                                    {{#contains [expose.expose].realEstate.cellar "YES"}}
                                        <span class="keyword">Keller</span>
                                    {{/contains}}
                                {{/if}}
                                {{#if [expose.expose].realEstate.handicappedAccessible}}
                                    {{#contains [expose.expose].realEstate.handicappedAccessible "YES"}}
                                        <span class="keyword">Stufenloser Zugang</span>
                                    {{/contains}}
                                {{/if}}
                                {{#if [expose.expose].realEstate.guestToilet}}
                                    {{#contains [expose.expose].realEstate.guestToilet "YES"}}
                                        <span class="keyword">Gäste-WC</span>
                                    {{/contains}}
                                {{/if}}
                                <h1>{{[expose.expose].realEstate.title}}</h1>
                                <sup>{{[expose.expose].realEstate.address.postcode}} {{[expose.expose].realEstate.address.city}} {{[expose.expose].realEstate.address.quarter}}</sup>
                                <p>{{[expose.expose].realEstate.descriptionNote}}</p>
                                <p>{{[expose.expose].realEstate.furnishingNote}}</p>
                            </div>
                            <div class="col x4">
                                <div class="keyfacts">
                                    <div class="keyfacts__item">
                                        <div class="keyfacts__label">
                                            Kaufpreis
                                        </div>
                                        <div class="keyfacts__keyfact">
                                            {{[expose.expose].realEstate.price.value}}€
                                        </div>
                                    </div>
                                    <div class="keyfacts__item">
                                        <div class="keyfacts__label">
                                            Wohnfläche
                                        </div>
                                        <div class="keyfacts__keyfact">
                                            {{[expose.expose].realEstate.livingSpace}}m²
                                        </div>
                                    </div>
                                    <div class="keyfacts__item">
                                        <div class="keyfacts__label">
                                            Zimmer
                                        </div>
                                        <div class="keyfacts__keyfact">
                                            {{[expose.expose].realEstate.numberOfRooms}}
                                        </div>
                                    </div>
                                    <div class="keyfacts__item">
                                        <div class="keyfacts__label">
                                            Grundstück
                                        </div>
                                        <div class="keyfacts__keyfact">
                                            {{[expose.expose].realEstate.plotArea}}m²
                                        </div>
                                    </div>
                                </div>
                                <a class="cta cta--details" href="immobilien.html">Anfrage starten</a>
                                {{#if [expose.expose].realEstate.numberOfFloors}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Etagen&shy;anzahl</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.numberOfFloors}}</dd>
                                    </dl>
                                {{/if}}
                                {{#if [expose.expose].realEstate.usableFloorSpace}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Nutz&shy;fläche ca.</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.usableFloorSpace}}m²</dd>
                                    </dl>
                                {{/if}}
                                {{#if [expose.expose].realEstate.numberOfBedRooms}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Schlaf&shy;zimmer</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.numberOfBedRooms}}</dd>
                                    </dl>
                                {{/if}}
                                {{#if [expose.expose].realEstate.numberOfBathRooms}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Bade&shy;zimmer</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.numberOfBathRooms}}</dd>
                                    </dl>
                                {{/if}}
                                {{#if [expose.expose].realEstate.parkingSpaceType}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Garage/ Stell&shy;platz</dt>
                                        {{#contains [expose.expose].realEstate.parkingSpaceType "GARAGE"}}
                                            <dd class="details__value">Garage</dd>
                                        {{/contains}}
                                    </dl>
                                {{/if}}
                                {{#if [expose.expose].realEstate.numberOfParkingSpaces}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Anzahl Garage/ Stell&shy;platz</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.numberOfParkingSpaces}}</dd>
                                    </dl>
                                {{/if}}



                                <div id="{{[expose.expose].realEstate.[@id]}}-map" class="details__map" data-lat="51.958827" data-lng="8.589504"></div>


                                {{#if [expose.expose].realEstate.constructionYear}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Baujahr</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.constructionYear}}</dd>
                                    </dl>
                                {{/if}}
                                <dl class="details clearfix">
                                    <dt class="details__label">Letzte Moderni&shy;sierung/ Sanierung</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.lastRefurbishment}}</dd>
                                </dl>
                                <dl class="details clearfix">
                                    <dt class="details__label">Objekt&shy;zustand</dt>
                                    {{#contains [expose.expose].realEstate.condition "MODERNIZED"}}
                                        <dd class="details__value">Modernisiert</dd>
                                    {{/contains}}
                                </dl>
                                {{#if [expose.expose].realEstate.interiorQuality}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Qualität der Aus&shy;stattung</dt>
                                        {{#contains [expose.expose].realEstate.interiorQuality "SOPHISTICATED"}}
                                            <dd class="details__value">Gehoben</dd>
                                        {{/contains}}
                                    </dl>
                                {{/if}}
                                <dl class="details clearfix">
                                    <dt class="details__label">Heizungs&shy;art</dt>
                                    {{#contains [expose.expose].realEstate.firingTypes.0.firingType "GAS"}}
                                        <dd class="details__value">Gas-Heizung</dd>
                                    {{/contains}}
                                </dl>
                                {{#if [expose.expose].realEstate.energyCertificate.energyEfficiencyClass}}
                                    <dl class="details clearfix">
                                        <dt class="details__label">Energie&shy;effizienz&shy;klasse</dt>
                                        <dd class="details__value">{{[expose.expose].realEstate.energyCertificate.energyEfficiencyClass}}</dd>
                                    </dl>
                                {{/if}}



                                <a class="cta cta--details" href="immobilien.html">Anfrage starten</a>



                                <dl class="details clearfix">
                                    <dt class="details__label">Provision für Käufer</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.courtage.courtage}}</dd>
                                </dl>



