                                    <dd class="details__value">{{[expose.expose].realEstate.livingSpace}}</dd>
                                    <dt class="details__label">Grundstücksfläche ca. </dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.plotArea}}</dd>
                                    <dt class="details__label">price</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.price.value}}</dd>
                                    <dd class="details__value">{{[expose.expose].realEstate.price.currency}}</dd>
                                    <dd class="details__value">{{[expose.expose].realEstate.price.marketingType}}</dd>
                                    <dd class="details__value">{{[expose.expose].realEstate.price.priceIntervalType}}</dd>
                                </dl>



                                -->
                            </div>
                        </div>
                    {{else}}
                    <div class="row">
                        <div class="col x9">
                            <p>Die Immobilie konnte nicht gefunden werden.</p>
                        </div>
                    </div>
                    {{/if}}
                </article>
