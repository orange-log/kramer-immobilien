                                    <dt class="details__label">energyConsumptionContainsWarmWater</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.energyConsumptionContainsWarmWater}}</dd>
                                    <dt class="details__label">floorplan</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.floorplan}}</dd>
                                    <dt class="details__label">constructionPhase</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.constructionPhase}}</dd>
                                    <dt class="details__label">buildingType</dt>
                                    <dd class="details__value">{{[expose.expose].realEstate.buildingType}}</dd>
